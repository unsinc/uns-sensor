/var/log/suricata/*.log /var/log/suricata/*.json /sns/IDS/suricata/logs/*.json /sns/IDS/suricata/logs/*.log 
{
    rotate 5
    daily
    missingok
    compress
    delaycompress
    create
    sharedscripts
    postrotate
            /bin/kill -HUP `cat /var/run/suricata.pid 2>/dev/null` 2>/dev/null || true
    endscript
}